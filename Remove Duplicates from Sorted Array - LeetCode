class Solution {
    public int removeDuplicates(int[] nums) {
        int[] dup = new int[nums.length];
        dup[0] = nums[0];int j=1;
        for(int i=1;i< nums.length;i++){
            if(nums[i] != nums[i-1]){
                dup[j] = nums[i];
                j++;
            }
        }
        for(int k=0;k < j;k++){
            nums[k] = dup[k];
        }
        return j;
    }
}
