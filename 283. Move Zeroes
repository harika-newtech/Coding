class Solution {
    public void moveZeroes(int[] nums) {
        int count =0;
        int temp;
        //count maintains count of non-zero elements
        for(int i=0;i< nums.length;i++){
            if(nums[i] != 0)
            {
                temp = nums[count];
                nums[count]= nums[i];
                nums[i] = temp;
                count++;
            }

        }

        /*for(int i=count;i< nums.length;i++){
            nums[i] =0;
        }*/
        
    }
}
